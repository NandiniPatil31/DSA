//Input: nums = [1,2,3,4]
//Output: [24,12,8,6]

//Sol 1: 
class Solution {
    public int[] productExceptSelf(int[] nums) {
        int[] answer = new int[nums.length];
        
        
        for(int i = 0 ; i< nums.length; i++){
            int total = 1;
            for(int j =0; j<nums.length; j++){
                if(i==j) continue;
                total = total * nums[j];
            }
            answer[i] = total;
        }
        return answer;
    }
}
